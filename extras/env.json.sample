{
        "modulePrefix": "lemonade-ember",
        "environment": "production",
        "rootURL": "/",
        "locationType": "auto",
        "EmberENV": {
                "FEATURES": {},
                "EXTEND_PROTOTYPES": {
                        "Date": false
                }
        },
        "APP": {
                "name": "lemonade-ember",
                "version": "0.0.0+"
        },
        "ember-simple-auth": {
                "authorizer": "authorizer:devise",
                "routeAfterAuthentication": "/home/workflows"
        },
        "thorn": "http://thorn",
        "stand": "http://stand",
        "tahiti": "http:/tahiti",
        "webSocketIO": {
                "url": "http://stand",
                "namespace": "/stand",
                "path": "/socket.io"
        },
        "exportApplicationGlobal": false
}

