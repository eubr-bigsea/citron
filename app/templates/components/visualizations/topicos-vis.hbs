<div class="gViz-header">
	<div class='symbol'>
		<span class="fa-stack fa-lg">
			<i class="fa fa-circle fa-stack-2x"></i>
			<i class="fa fa-bar-chart fa-stack-1x fa-inverse"></i>
		</span>
	</div>
	<span class='title'>{{title}}</span>
</div>

<div class='gViz-wrapper' data-id={{_id}} style={{style}} >
  <table class="table table-striped table-hover gViz-table">
    <thead>
      <tr>
        {{#each header as |h i|}}
          <th>{{{h}}}</th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each tableData as |tr|}}
        <tr>
          {{#each tr as |td|}}
            <td>{{{td}}}</td>
          {{/each}}
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
