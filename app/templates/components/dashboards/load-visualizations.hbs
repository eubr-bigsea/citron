<div class='header'>
  <p class='title'>{{model.title}}</p>
  <div class='actions-custom'>
    <button class='btn btn-sm' {{action 'save'}}><i class='fa fa-save'></i></button>
  </div>
</div>
<div class="grid-stack">
  {{#if isEmpty}}
    Loading data...
  {{else }}
    {{#each visualizations as |viz|}}
      {{dashboards/vis-wrapper viz=viz dataUrl=viz.dataUrl  config=model.configuration}}
    {{/each }}
  {{/if}}
</div>
