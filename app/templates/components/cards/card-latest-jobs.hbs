<div class="card" id='release-notes'>
  <div class="card-header">
    <div class="header-block">
      <h3 class="title">{{card.title}}</h3>
    </div>
  </div>
  <div class="card-block" id="large-table-jobs" >
    <table class="table table-hover" >
      <thead>
        <tr>
          <th>{{t 'tables.id'}}</th>
          <th>{{t 'workflow'}}</th>
          <th>{{t 'tables.status'}}</th>
          <th>{{t 'tables.result'}}</th>
        </tr>
      </thead>
      <tbody>
        {{#each jobs as |job|}}
          {{cards/item-list-jobs job=job}}
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
