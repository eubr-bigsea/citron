<div class="container-fluid" style="margin-left:400px">
<section class="col-xs-12 col-sm-12 col-md-12 col-lg-4 col-xl-3 name-editor">
  {{option-bar.name-editor workflow=workflow}}
</section>
<section class="col-xs-12 col-sm-12 col-md-6 col-lg-5 col-xl-2 option-bar">
  {{option-bar.option-buttons workflow=workflow}}
</section>
<section class="col-xs-12  col-sm-12 col-md-6 col-lg-3 col-xl-3 execute-bar">
  <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 execute-bar execute-btn">
    <a onclick={{action 'play'}} class="play btn btn-md rounded-s	execute-bar execute-btn">
      <i class="fa fa-play"></i>
    </a>
  </div>
</section>
</div>
