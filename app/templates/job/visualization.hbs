<div class="main-wrapper">
  <div class="app sidebar-fixed header-fixed footer-fixed" id="app">
    {{workflow.config-sidebar}}
    <article class="content dashboard-page">
      <div class="container-fluid">
        <div class="row">
          <div class="col-xs-12">
            <div class="title-block">
              <h3 class="title">Visualization
                <span class="sparkline bar" data-type="bar"></span>
              </h3>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12">
            <a href="javascript:history.back()">Back to results</a>
          </div>
          <div class="col-xs-12">
            {{component model.component
            width="100%"
            height="600px"
            _id="uniq-visu"
            title="Visualization"
            dataUrl=model.url
            }}
          </div>
        </div>
      </div>
    </article>
  </div>
</div>
<!-- Tooltipster markup -->
<div class='tooltipster-visualization tooltipster-base tooltipster-sidetip tooltipster-top tooltipster-fade tooltipster-show'>
  <div class='tooltipster-box'>
    <div class='tooltipster-content'></div>
  </div>
  <div class='tooltipster-arrow'>
    <div class='tooltipster-arrow-uncropped'>
      <div class='tooltipster-arrow-border'></div>
      <div class='tooltipster-arrow-background'></div>
    </div>
  </div>
</div>

<!-- Loading markup -->
<div class="loading-div">
  <div class="loading-div-inner">
    <div class="loader">
      <i class='fa fa-spinner fa-pulse fa-3x'></i>
    </div>
  </div>
</div>
