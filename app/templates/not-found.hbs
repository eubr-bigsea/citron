<div class="job not-found row">
  <div class="col-xs-12 col-sm-12 col-md-6">
    <h1 class="title">Oops!</h1>
    <p class="message">{{t 'notFound.message' resource=resource}}</p>
    {{#link-to resources class="btn btn-primary"}}
      {{t 'notFound.goBack'}} {{resources}}
    {{/link-to}}
  </div>
  <div class="col-xs-12 col-sm-12 col-md-6">
    <img src="/assets/images/lemons_are_sad.png"/>
  </div>
</div>
